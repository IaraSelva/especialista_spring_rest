package exemplo;

import java.math.BigDecimal;

public class Produto {
	
	private String nome;
	private BigDecimal valorTotal;
	
	
	public Produto(String nome, double d) {
		super();
		this.nome = nome;
		this.valorTotal = d;
	}
	
	public String getNome() {
		return nome;
	}
	
	public BigDecimal getValorTotal() {
		return valorTotal;
	}

}
